{
  "displayName": "Homebridge Fibaro",
  "name": "homebridge-fibaro",
  "version": "1.3.0",
  "description": "Homebridge plugin for Fibaro Home Center",
  "author": "oxystin",
  "license": "Apache-2.0",
  "homepage": "https://github.com/oxystin/homebridge-fibaro",
  "repository": {
